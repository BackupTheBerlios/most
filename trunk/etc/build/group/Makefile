-include $(MOST_ROOT)/etc/build/Commands

.PHONY : default help obj clean mrproper

default:
	@$(ECHO) "Please use <make help> for more information."

help :
	@$(call makegoal, $@)
	@$(ECHO) "---------------------- build/group/Makefile -------------------------------------------------------"
	@$(ECHO) "help     : print this help message, -> Makefile"
	@$(ECHO) "obj      : -> Makefile"
	@$(ECHO) "clean    : -> Makefile"
	@$(ECHO) "mrproper : -> Makefile" 
	@$(ECHO) "---------------------------------------------------------------------------------------------"

obj :
	@$(call makegoal, $@)

clean : 
	@$(call makegoal, $@)

mrproper : 
	@$(call makegoal, $@)
